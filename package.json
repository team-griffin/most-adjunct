{
  "name": "most-adjunct",
  "version": "1.1.0",
  "main": "dist/cjs/most-adjunct.js",
  "module": "dist/es/most-adjunct.js",
  "types": "dist/es/index.d.ts",
  "repository": "git@github.com:team-griffin/most-adjunct.git",
  "author": "Joshua Thornton <joshua.thornton@heg.com>",
  "license": "MIT",
  "scripts": {
    "prepublishOnly": "cross-env NODE_ENV=production yarn build",
    "lint": "yarn lint:js && yarn lint:ts",
    "lint:js": "eslint -c ./.eslintrc.js 'src/**/*.ts'",
    "lint:ts": "tsc",
    "build": "rollup --config ./rollup.config.js",
    "test": "cross-env NODE_ENV=test TS_NODE_FILES=true mocha --require \"@babel/register\" --require \"./test-setup.js\" \"src/**/*.test.{js,ts}\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint --edit"
    }
  },
  "lint-staged": {
    "linters": {
      "src/**/*.{js,ts,tsx}": [
        "eslint -c ./.eslintrc.js",
        "git add"
      ]
    }
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/register": "^7.5.5",
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-conventional": "^8.1.0",
    "@semantic-release/changelog": "^3.0.2",
    "@team-griffin/eslint-config": "^3.2.0",
    "@types/chai": "^4.1.7",
    "@types/mocha": "^8.0.3",
    "@types/sinon": "^9.0.5",
    "@typescript-eslint/eslint-plugin": "^4.1.0",
    "@typescript-eslint/parser": "^4.1.0",
    "browser-env": "^3.2.6",
    "chai": "^4.0.2",
    "cross-env": "^7.0.2",
    "eslint": "^7.8.1",
    "eslint-plugin-typescript": "^0.14.0",
    "husky": "^3.0.2",
    "lint-staged": "^8.0.0",
    "mocha": "^8.1.3",
    "most": "^1.5.1",
    "rollup": "^2.26.11",
    "rollup-plugin-babel": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-typescript2": "^0.27.2",
    "semantic-release": "^17.2.3",
    "sinon": "^9.0.3",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "@most/create": "^2.0.1"
  },
  "peerDependencies": {
    "most": "^1.5.1"
  }
}
