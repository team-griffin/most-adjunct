{
  "name": "most-adjunct",
  "version": "0.12.4",
  "main": "dist/cjs/most-adjunct.js",
  "module": "dist/es/most-adjunct.js",
  "repository": "git@github.com:team-griffin/most-adjunct.git",
  "author": "Joshua Thornton <joshua.thornton@heg.com>",
  "license": "MIT",
  "scripts": {
    "prepublishOnly": "npm run build",
    "lint": "npm run lint:js",
    "lint:js": "eslint -c ./config/.eslintrc.js 'src/**/*.js'",
    "lint:js:fix": "eslint --fix -c ./config/.eslintrc.js 'src/**/*.js'",
    "flow": "flow check; test $? -eq 0 -o $? -eq 2",
    "build": "rollup --config ./rollup/config.js",
    "test": "cross-env NODE_ENV=test nyc mocha",
    "postmerge": "yarn"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "^6.1.21",
    "babel-eslint": "^8.0.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "chai": "^4.0.2",
    "cross-env": "^5.0.1",
    "eslint": "^4.13.0",
    "eslint-plugin-flowtype": "^2.30.4",
    "mocha": "^4.0.1",
    "nyc": "^11.0.2",
    "rollup": "^0.54.0",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-local-resolve": "^1.0.7"
  },
  "nyc": {
    "require": [
      "babel-register",
      "babel-polyfill"
    ],
    "sourceMap": false,
    "instrument": false,
    "include": [
      "src/**/*.js"
    ]
  },
  "dependencies": {
    "@most/create": "^2.0.1",
    "@team-griffin/capra": "^0.4.1",
    "most": "^1.5.1",
    "mostc": "^1.0.2",
    "ramda": "^0.x.x"
  }
}
